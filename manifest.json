{
  "manifest_version": 2,
  "name": "TabHistory",
  "version": "0.1.0",

  "permissions": [
          "tabs",
          "storage"
      ],

  //"default_locale": "en",
  "description": "Save currently opened tabs",
  "icons": {"16": "img/tabhistory_16.png",
            "48": "img/tabhistory_48.png",
            "128": "img/tabhistory_128.png"},
  "browser_action": {
    "default_title": "TabHistory",
    "default_icon": "img/tabhistory_16.png",
    "default_popup": "popup.html"
  },

  "background":{
      "scripts":["js/background.js"]
  },

  "author": "Nick Robinson"
}
